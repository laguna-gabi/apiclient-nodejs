{
  "name": "hepius",
  "version": "0.12.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:dev": "export NODE_ENV=development && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:all": "docker-compose up -d && yarn start",
    "lint:fix": "eslint \"{src,cmd,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,cmd,apps,libs,test}/**/*.ts\"",
    "seed": "ts-node scripts/seed.ts",
    "seed:dev": "export NODE_ENV=development && ts-node scripts/seed.ts",
    "seed:laguna": "export NODE_ENV=development && ts-node scripts/seedLagunaData.ts",
    "seed:create:users:dev": "export NODE_ENV=development && ts-node scripts/seedNewUser.ts",
    "seed:create:users:production": "export NODE_ENV=production && ts-node scripts/seedNewUser.ts",
    "generate:tokens": "ts-node scripts/generateTokens.ts",
    "generate:tokens:dev": "export NODE_ENV=development && ts-node scripts/generateTokens.ts",
    "generate:tokens:production": "export NODE_ENV=production && ts-node scripts/generateTokens.ts",
    "test": "jest --config test/unit/jest.json",
    "test:watch": "jest --config test/unit/jest.json --watch",
    "test:cov": "jest --config test/jest.json --logHeapUsage && jest-coverage-badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config test/integration/jest.json",
    "test:validation": "jest --config test/validations/jest.json",
    "test:live": "jest --config test/live/jest.json",
    "test:all": "yarn test && yarn test:validation && yarn test:integration",
    "analytics:dump": "ts-node ./cmd/main.ts analytics",
    "analytics:dump:dev": "export NODE_ENV=development && ts-node ./cmd/main.ts analytics",
    "analytics:dump:prod": "export NODE_ENV=production && ts-node ./cmd/main.ts analytics",
    "localstack": "localstack/configLocalstack.sh",
    "migrate": "ts-node ./cmd/main.ts migration",
    "setup": "./scripts/setup.sh"
  },
  "dependencies": {
    "@lagunahealth/pandora": "0.11.5",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.3.0",
    "@nestjs/core": "^8.3.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.1.1",
    "@nestjs/platform-express": "^8.3.0",
    "@nestjs/terminus": "^8.0.4",
    "@slack/webhook": "^6.1.0",
    "@splitsoftware/splitio": "^10.17.2",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.1073.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-table3": "^0.6.1",
    "config": "^3.3.7",
    "cron-validator": "^1.3.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "commander": "^8.3.0",
    "download": "^8.0.0",
    "form-data": "^4.0.0",
    "graphql": "^15.5.2",
    "graphql-type-json": "^0.3.2",
    "json-2-csv": "^3.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.1",
    "mongoose-delete": "^0.5.4",
    "nest-commander": "^2.4.0",
    "nestjs-pino": "^2.5.0",
    "passport": "^0.5.2",
    "passport-custom": "^1.1.1",
    "pino-http": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "sharp": "^0.30.1",
    "sqs-consumer": "^5.6.0",
    "twilio": "^3.74.0",
    "uuid4": "^2.0.2",
    "zipcode-to-timezone": "^0.0.9"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/testing": "^8.3.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.17",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.1.2",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
