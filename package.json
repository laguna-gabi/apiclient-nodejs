{
  "name": "hepius",
  "version": "0.6.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:dev": "export NODE_ENV=development && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:all": "docker-compose up -d && yarn start",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "seed": "ts-node scripts/seed.ts",
    "seed:dev": "export NODE_ENV=development && ts-node scripts/seed.ts",
    "seed:laguna": "export NODE_ENV=development && ts-node scripts/seedLagunaData.ts",
    "seed:create:users:dev": "export NODE_ENV=development && ts-node scripts/seedNewUser.ts",
    "seed:create:users:production": "export NODE_ENV=production && ts-node scripts/seedNewUser.ts",
    "generate:tokens": "ts-node scripts/generateTokens.ts",
    "generate:tokens:dev": "export NODE_ENV=development && ts-node scripts/generateTokens.ts",
    "generate:tokens:production": "export NODE_ENV=production && ts-node scripts/generateTokens.ts",
    "test": "jest --config test/unit/jest.json",
    "test:watch": "jest --config test/unit/jest.json --watch",
    "test:cov": "jest --config test/jest.json --logHeapUsage && jest-coverage-badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config test/integration/jest.json",
    "test:validation": "jest --config test/validations/jest.json",
    "test:live": "jest --config test/live/jest.json",
    "test:all": "yarn test && yarn test:validation && yarn test:integration",
    "migrate:init": "ts-node ./migrations/generateConfig.ts",
    "migrate:create": "cd migrations && migrate-mongo create",
    "migrate:status": "cd migrations && migrate-mongo status",
    "migrate:up": "cd migrations && migrate-mongo up",
    "migrate:down": "cd migrations && migrate-mongo down"
  },
  "dependencies": {
    "@lagunahealth/pandora": "0.4.4",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/terminus": "^7.2.0",
    "@slack/webhook": "^6.0.0",
    "@splitsoftware/splitio": "^10.16.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.172",
    "@types/mongoose": "^5.11.97",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.983.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.6",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.1.6",
    "graphql": "^15.5.2",
    "graphql-type-json": "^0.3.2",
    "i18next": "^21.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.14",
    "passport": "^0.5.0",
    "passport-custom": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "twilio": "^3.67.1",
    "uuid4": "^2.0.2",
    "zipcode-to-timezone": "^0.0.9"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^7.6.15",
    "@types/faker": "^5.5.6",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "migrate-mongo": "^8.2.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
