{
  "root": "apps/poseidon/client",
  "sourceRoot": "apps/poseidon/client/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/poseidon/client/**/*.ts"]
      }
    },
    "test:cov": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/poseidonClient"],
      "options": {
        "jestConfig": "apps/poseidon/client/test/jest.config.js",
        "passWithNoTests": true
      }
    },
    "cov-badges": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "jest-coverage-badges input coverage/libs/poseidonClient/coverage-summary.json output coverage/libs/poseidonClient"
      }
    },
    "uploadCoverage": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P ./tsconfig.base.json ./ci/uploadCoverage.ts poseidonClient coverage/libs/poseidonClient/"
      }
    }
  },
  "tags": []
}
