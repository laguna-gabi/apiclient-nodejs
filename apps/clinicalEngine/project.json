{
  "root": "apps/clinicalEngine",
  "sourceRoot": "apps/clinicalEngine/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/clinicalEngine",
        "main": "apps/clinicalEngine/src/main.ts",
        "tsConfig": "apps/clinicalEngine/tsconfig.app.json",
        "webpackConfig": "./webpackConfig.js",
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "clinicalEngine:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/clinicalEngine/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/clinicalEngine/test/unit/jest.config.js",
        "passWithNoTests": true
      }
    },
    "test:cov": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/clinicalEngine"],
      "options": {
        "jestConfig": "apps/clinicalEngine/test/jest.config.js",
        "passWithNoTests": true
      }
    },
    "cov-badges": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "jest-coverage-badges input coverage/apps/clinicalEngine/coverage-summary.json output coverage/apps/clinicalEngine"
      }
    }
  },
  "tags": []
}
