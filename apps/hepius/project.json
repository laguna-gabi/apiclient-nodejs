{
  "root": "apps/hepius",
  "sourceRoot": "apps/hepius/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/hepius",
        "main": "apps/hepius/src/main.ts",
        "tsConfig": "apps/hepius/tsconfig.json",
        "webpackConfig": "./webpackConfig.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/hepius/src/environments/environment.ts",
              "with": "apps/hepius/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "hepius:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/hepius/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/hepius"],
      "options": {
        "jestConfig": "apps/hepius/jest.config.js",
        "passWithNoTests": true
      }
    },
    "test:cov": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "jest --config apps/hepius/test/jest.json --logHeapUsage && jest-coverage-badges"
      }
    }
  },
  "tags": []
}
