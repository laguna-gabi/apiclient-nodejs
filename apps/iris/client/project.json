{
  "root": "apps/iris/client",
  "sourceRoot": "apps/iris/client/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/iris/client/**/*.ts"]
      }
    },
    "test:cov": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/irisClient"],
      "options": {
        "jestConfig": "apps/iris/client/test/jest.config.js",
        "passWithNoTests": true
      }
    },
    "cov-badges": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "jest-coverage-badges input coverage/libs/irisClient/coverage-summary.json output coverage/libs/irisClient"
      }
    },
    "uploadCoverage": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "ts-node -P ./tsconfig.base.json ./ci/uploadCoverage.ts irisClient coverage/libs/irisClient/"
      }
    }
  },
  "tags": []
}
